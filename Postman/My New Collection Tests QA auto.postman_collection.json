							"    });\r",
							"}\r",
							"else{\r",
							"    console.log(\"Test failed: \", pm.response.code);\r",
							"    throw new Error(\"Test failed\")\r",
							"}\r",
							"\r",
							"\r",
							"var schema = {\r",
							"    \"type\": \"object\",\r",
							"  \"properties\": {\r",
